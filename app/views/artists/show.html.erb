<h1>Artist <br/> <%=@artist.name %></h1>
<hr />
<h2> Top Hits </h2>


<ul>
    <% @artist.songs.each do |song| %>
  <li>
    <%= song.title %>
  </li>
  <% end %>
</ul>
<ul>

    <% @artist.billboards.each do |billboard| %>
  <li>
    <%= link_to billboard.title, billboard%>
  </li>
  <% end %>
</ul>
<br />
  <%= link_to 'Delete Artist', @artist, method: :delete %>